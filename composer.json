{
    "name": "lusitanian/oauth",
    "description": "PHP 5.3+ oAuth 1/2 Library",
    "keywords": ["oauth", "authentication", "authorization", "security"],
    "license": "MIT",
    "authors": [
        {
            "name": "David Desberg",
            "email": "david@daviddesberg.com"
        },
        {
            "name": "Pieter Hordijk",
            "email": "info@pieterhordijk.com"
        },
        {
            "name": "Elliot Chance",
            "email": "elliotchance@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.3.*"
